<h1>会員一覧</h1>
<table class="table table-striped">
    <th>会員ID</th>
    <th>氏名</th>
    <th>メールアドレス</th>
    <th>ステータス</th>
    <% @customers.each do |customer| %>
        <tr>
        <td><%= customer.id %></td>
        <td><%= customer.name %></td>
        <td><%= customer.email %></td>
        <td><%= customer.is_deleted %></td>
        </tr>
    <% end %>
</table>